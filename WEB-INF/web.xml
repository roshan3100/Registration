<web-app>    
<servlet>
	<servlet-name>Roshan</servlet-name>
	<servlet-class>servletclass.register</servlet-class>
</servlet>
<servlet>
	<servlet-name>info</servlet-name>
	<servlet-class>servletclass.info</servlet-class>
</servlet>
<servlet>
	<servlet-name>profile</servlet-name>
	<servlet-class>servletclass.WelcomeServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>logout</servlet-name>
	<servlet-class>servletclass.logout</servlet-class>
</servlet>
<servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>servletclass.login</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>info</servlet-name>
	<url-pattern>/info</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>profile</servlet-name>
	<url-pattern>/profile</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>logout</servlet-name>
	<url-pattern>/logout</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>Roshan</servlet-name>
	<url-pattern>/register</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/verify</url-pattern>
</servlet-mapping>
</web-app>